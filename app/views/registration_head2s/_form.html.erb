<div class="row">
<div class="col-md-12">
    <p id="notice"><%= notice %></p>
    <div class="callout callout-warning">
                <h4> 
<%=Championship.where(:id => params[:id]).first.name%></h4>
    </div>
       </div>
     </div>
    <div class="row">
      <div class="col-md-2">
      </div>
       <div class="col-md-8">
<%= simple_form_for(@registration_head2) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :championship, :as => :hidden, :input_html => { :value =>  params[:id]}%>
    <%= f.association :sex%>
   <%= f.input :fecha, :as => :hidden, :input_html => { :value =>  Time.now}%>
    <%= f.input :name %>
    <%= f.input :lastname %>
    <%= f.date_field :an , class: 'form-control'%>
    <%= f.input :club %>
    <%= f.input :region %>
    <%= f.input :pais %>
    <%= f.input :dni %>
  </div>



 <div class="form-actions">
    <%= f.button :submit, value: "SIGUIENTE", class: "btn btn-success" %>
  </div>
<% end %>
</div>
</div>
 <div class="col-md-2">
 </div>
</div>