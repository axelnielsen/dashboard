<p id="notice"><%= notice %></p>

<div class="col-md-12">
    <div class="callout callout-info">
      <h4><strong>Lanzamiento Bala - Varones  </strong></h4>
     &#09;<h5>Insertar_Fecha</h5>
     </div>

   <% if  user_signed_in? && current_user.email=='admin@prossport.cl'%>
       <div class="row">
          <div class="col-xs-12">
       
             
              <!-- /.box-header -->
              <div class="box-body table-responsive no-padding">
                <table class="table table-hover">
    <%= simple_form_for(@male_shot_put) do |f| %>
    <%= f.error_notification %>
                  <tr>
                    <th><%= f.input :place,  placeholder: "Lugar",label: false%></th>
                    <th> <%= f.input :name , placeholder: "nombre",label: false%></th>
                    <th><%= f.input :first , placeholder: "primero",label: false%></th>
                    <th> <%= f.input :second, placeholder: "segundo" ,label: false%></th>
                    <th><%= f.input :third , placeholder: "tercero",label: false%></th>
                    <th><%= f.input :fourth , placeholder: "cuarto",label: false%></th>
                    <th><%= f.input :fifth , placeholder: "quinto",label: false%></th>
                    <th> <%= f.input :sixth, placeholder: "sexto",label: false%></th>
                     <th> <%= f.input :achievement, placeholder: "Marca",label: false%></th>
                     
                    <th> <%= f.button :submit ,'Guardar',class: "btn btn-success"%></th>
                  </tr>
                  <%end%>
                         </table>
              </div>
              <!-- /.box-body -->
           
            <!-- /.box -->
          </div>
        </div>
  <%end%>
</div>

<%= content_tag :table, role: :datatable, data: { url: male_shot_puts_path(format: :json)} do %>
  <thead>
    <tr>
      <th>Número</th>
      <th>Nombre</th>
      <th>A.N</th>
      <th>Club</th>
      <th>Región</th>
      <th>Primero</th>
      <th>Segundo</th>
      <th>Tercero</th>
      <th>Cuarto</th>
      <th>Quinto</th>
      <th>Sexto</th>
      <th>Marca</th>
      <th>Lugar</th>
      <th colspan="3"></th>
    </tr>
  </thead>

   
 <tbody onmouseover="bestInPlace();">
  </tbody>
<% end %>

 <% if  user_signed_in? && current_user.email=='admin@prossport.cl'%>
<script type="application/javascript">
$(document).ready(function() {
jQuery(".best_in_place").best_in_place();
});
function bestInPlace()
{
 jQuery(".best_in_place").best_in_place();
};
 </script>
    <br><br><br><br><br>
<div class="col-md-12">
    <div class="callout callout-success">
      <h4><strong>IMPORTAR</strong></h4>
     </div>
   <%=form_tag import_male_shot_puts_path, multipart: true do%>
<%=file_field_tag :file%>
<%=submit_tag "Importar"%>
 <% end %>
</div>
</div>
 <%end%>
 