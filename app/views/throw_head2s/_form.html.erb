
<%= simple_form_for(@throw_head2) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :fecha_date %>
    <%= f.input :hora %>
    <%= f.association :sex %>
    <%= f.association :competition %>
    <%= f.association :sport %>
    <%= f.association :category %>
    <%= f.input :type %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
