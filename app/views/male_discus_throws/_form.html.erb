
<%= simple_form_for(@male_discus_throw) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :number %>
    <%= f.input :name %>
    <%= f.input :an %>
    <%= f.input :club %>
    <%= f.input :region %>
    <%= f.input :first %>
    <%= f.input :second %>
    <%= f.input :third %>
    <%= f.input :fourth %>
    <%= f.input :fifth %>
    <%= f.input :sixth %>
    <%= f.input :achievement %>
    <%= f.input :place %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
