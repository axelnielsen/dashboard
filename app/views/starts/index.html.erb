<p id="notice"><%= notice %></p>

<h1>Starts</h1>

<table>
  <thead>
    <tr>
      <th>Fecha</th>
      <th>Hora</th>
      <th>Sex</th>
      <th>Competition</th>
      <th>Sport</th>
      <th>Category</th>
      <th>Serie</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @starts.each do |start| %>
      <tr>
        <td><%= start.fecha %></td>
        <td><%= start.hora %></td>
        <td><%= start.sex %></td>
        <td><%= start.competition %></td>
        <td><%= start.sport %></td>
        <td><%= start.category %></td>
        <td><%= start.serie %></td>
        <td><%= link_to 'Show', start %></td>
        <td><%= link_to 'Edit', edit_start_path(start) %></td>
        <td><%= link_to 'Destroy', start, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Start', new_start_path %>
