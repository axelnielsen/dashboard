<%= content_tag :table, role: :datatable, data: { url: female_discus_throws_path(format: :json)} do %>
  <thead>
    <tr>
      <th>Number</th>
      <th>Name</th>
      <th>An</th>
      <th>Club</th>
      <th>Region</th>
      <th>First</th>
      <th>Second</th>
      <th>Third</th>
      <th>Fourth</th>
      <th>Fifth</th>
      <th>Sixth</th>
      <th>Achievement</th>
      <th>Place</th>
      <th colspan="3"></th>
    </tr>
  </thead>

 
 <tbody onmouseover="bestInPlace();">
  </tbody>
<% end %>

 
<script type="application/javascript">
$(document).ready(function() {
jQuery(".best_in_place").best_in_place();
});
function bestInPlace()
{
 jQuery(".best_in_place").best_in_place();
};

</script>
