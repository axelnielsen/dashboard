<p id="notice"><%= notice %></p>

<h1>Championships</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Initdate</th>
      <th>Findate</th>
      <th>Idregion</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @championships.each do |championship| %>
      <tr>
        <td><%= championship.name %></td>
        <td><%= championship.initdate %></td>
        <td><%= championship.findate %></td>
        <td><%= championship.idRegion %></td>
        <td><%= championship.address %></td>
        <td><%= link_to 'Show', championship %></td>
        <td><%= link_to 'Edit', edit_championship_path(championship) %></td>
        <td><%= link_to 'Destroy', championship, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Championship', new_championship_path %>
