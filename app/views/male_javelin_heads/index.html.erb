<p id="notice"><%= notice %></p>

<h1>Male Javelin Heads</h1>

<table>
  <thead>
    <tr>
      <th>Championship</th>
      <th>Fecha</th>
      <th>Hora</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @male_javelin_heads.each do |male_javelin_head| %>
      <tr>
        <td><%= male_javelin_head.championship_id %></td>
        <td><%= male_javelin_head.fecha %></td>
        <td><%= male_javelin_head.hora %></td>
        <td><%= link_to 'Show', male_javelin_head %></td>
        <td><%= link_to 'Edit', edit_male_javelin_head_path(male_javelin_head) %></td>
        <td><%= link_to 'Destroy', male_javelin_head, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Male Javelin Head', new_male_javelin_head_path %>
