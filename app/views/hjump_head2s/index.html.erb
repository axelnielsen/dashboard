<div class="col-xs-12">
<p id="notice"><%= notice %></p>

<%
competition_id=params[:competition_id]

sex_id=1
sport_id=1
category_id=1
%>
<% @hjump_head2s.where(:competition_id =>competition_id
).each do |hjump_head2| %>
<%

competition_id=hjump_head2.competition_id
sex_id=hjump_head2.sex_id
sport_id=hjump_head2.sport_id
category_id=hjump_head2.category_id

%>
  <div class="row">
        <div class="col-xs-12">
          <div class="box">
            <div class="box-header table-info">
              <h3 class="box-title">Serie <% if  user_signed_in? && current_user.email=='admin@prossport.cl'%> <%=best_in_place(hjump_head2, :serie) %>
              <%else%>
              <%=hjump_head2.serie%>
                <%end%>
 
              </h3>

              <div class="box-tools">
             </div>
            </div>





     <% if  user_signed_in? && current_user.email=='admin@prossport.cl'%>
       <div class="row">
          <div class="col-xs-12">
       
             
              <!-- /.box-header -->
              <div class="box-body table-responsive no-padding">
                <table class="table table-hover">
    <%= simple_form_for :hjump2, :url => hjump2s_path, :method => :post  do |f| %>
    <%= f.error_notification %>
    <%= f.input :hjump_head2_id, :as => :hidden, :input_html => { :value => hjump_head2.id  }%>
                  <tr>
          
                    <th><%= f.input :athlete,  placeholder: "Atleta"%></th>
                    <th><%= f.input :club,  placeholder: "Club"%></th>
                     <th><%= f.input :an,  placeholder: "A.N."%></th>
                      <th><%= f.input :region,  placeholder: "Región"%></th>

                                <th> <%= f.input :achievement, placeholder: "Marca"%></th>
                                  <th> <%= f.input :place, placeholder: "Lugar"%></th>
                    <th> <%= f.button :submit ,'Guardar',class: "btn btn-success"%></th>
                  </tr>
                  <%end%>
                         </table>
              </div>
              <!-- /.box-body -->
           
            <!-- /.box -->
          </div>
        </div>
  <%end%>







            <!-- /.box-header -->
            <div class="box-body table-responsive no-padding table-striped">
              <table class="table table-hover">
                <tbody><tr class="table-info">
                  <% if  user_signed_in? && current_user.email=='admin@prossport.cl'%>
                  <th>Lugar</th>
              <th>Marca</th>
                  <th>Nombre</th>
                  <th>A.N.</th>
                  <th>Club</th>
                  <th>Región</th>
                  
                  <%else%>
                  <th>Lugar</th>

                  <th>Marca</th>
                  <th>Nombre</th>
                  <th>A.N.</th>
                  <th>Club</th>
                  <th>Región</th>
                  
                  <%end%>
                  
                   <% if  user_signed_in? && current_user.email=='admin@prossport.cl'%>
                   <th>ELIMINAR</th>
                   <%end%>
                </tr>
                  <%  hjump_head2.hjump2s.where(:hjump_head2_id => hjump_head2.id).order(place: :asc).each do |hjump2| %>
               
     
        <% if  user_signed_in? && current_user.email=='admin@prossport.cl'%>
  <tr>
<td><%= best_in_place(hjump2, :place) %></td>
        <td><%= best_in_place(hjump2, :achievement) %></td>
        <td><%= best_in_place(hjump2, :athlete) %></td>
        <td><%= best_in_place(hjump2, :an) %></td>
        <td><%= best_in_place(hjump2, :club) %></td>
         <td><%= best_in_place(hjump2, :region) %></td>
       
        
         <% if  user_signed_in? && current_user.email=='admin@prossport.cl'%>
        <td><%= link_to hjump2, method: :delete, data: { confirm: 'Está seguro?' }, class: "" do %><button type="button" class="btn btn-danger" aria-label="Left Align">X</button></td>
        <% end %> 
        <%end%>
      </tr>

 <%   else %>

<tr>
        <td><%= hjump2.place%></td>
        <td><%= hjump2.achievement %></td>
        <td><%=hjump2.athlete %></td>
       <td><%=hjump2.an %></td>
        <td><%= hjump2.club%></td>
        <td><%= hjump2.region %></td>
      </tr>



 <%end%>
    <% end %>
              </tbody></table>
            </div>
 <% if  user_signed_in? && current_user.email=='admin@prossport.cl'%>
    <div class="box-header callout-success">
      <h4><strong>IMPORTAR</strong></h4>
     </div>
   <%=form_tag import_hjump2s_path("id"=>hjump_head2.id), multipart: true do%>
<%=file_field_tag :file%>
<%=submit_tag "Importar"%>
 <% end %>
<%end%>

            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
      </div>


    
  <%end%>
    
<br>


  <% if  user_signed_in? && current_user.email=='admin@prossport.cl'%>
  <script type="application/javascript">

$(document).ready(function() {
jQuery(".best_in_place").best_in_place();
});
function bestInPlace()
{
 jQuery(".best_in_place").best_in_place();
};
 </script>
 <div class="box-header table-info">
              <h3 class="box-title">Nueva Serie
             
              </h3>

              <div class="box-tools">
              </div>
            </div>
       <div class="row">
          <div class="col-xs-12">
       
             
