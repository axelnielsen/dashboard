
<%= simple_form_for(@track_head) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :fecha %>
    <%= f.input :hora %>
    <%= f.input :gender %>
    <%= f.input :trackSerieId %>
    <%= f.input :sportId %>
    <%= f.input :categoryId %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
